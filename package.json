{
  "name": "hatool",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve hatool-tester",
    "lint": "ng lint",
    "build_lib": "ng build hatool --prod",
    "watch_lib": "ng build hatool --watch",
    "copy-license": "cp ./LICENSE ./dist/hatool",
    "copy-readme": "cp ./README.md ./dist/hatool",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/hatool && npm pack",
    "package": "npm run build_lib && npm run copy-files",
    "publish": "npm run package && cd dist/hatool && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.0",
    "@angular/common": "^13.2.0",
    "@angular/compiler": "^13.2.0",
    "@angular/core": "^13.2.0",
    "@angular/forms": "^13.2.0",
    "@angular/platform-browser": "^13.2.0",
    "@angular/platform-browser-dynamic": "^13.2.0",
    "@angular/router": "^13.2.0",
    "highlight.js": "^10.4.1",
    "ngx-highlightjs": "^4.1.2",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.5",
    "@angular/cli": "^13.2.5",
    "@angular/compiler-cli": "^13.2.0",
    "@types/highlight.js": "^9.12.4",
    "@types/jasmine": "^3.10.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.5.2"
  }
}
